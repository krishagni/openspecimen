        <foreignKey>IDENTIFIER</foreignKey>
        <parentKey>PARENT_SPECIMEN_ID</parentKey>
        <row>
          <textField>
            <name>parentId</name>
            <udn>parentId</udn>
            <caption>#getMessage("specimen_parent_specimen_id")</caption>
            <column>IDENTIFIER</column>
            <recordUrl>#/object-state-params-resolver?stateName=ParticipantsListItemSpecimenDetail.Overview&amp;objectName=specimen&amp;key=id&amp;value={{$value}}</recordUrl>
          </textField>
          <textField>
            <name>parentLabel</name>
            <udn>parentLabel</udn>
            <caption>#getMessage("specimen_parent_specimen_label")</caption>
            <column>LABEL</column>
            <recordUrl>#/object-state-params-resolver?stateName=ParticipantsListItemSpecimenDetail.Overview&amp;objectName=specimen&amp;key=id&amp;value={{$parentSpecimenId}}</recordUrl>
          </textField>
