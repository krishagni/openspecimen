            <udn>createdBy</udn>
            <caption>#getMessage("specimen_created_by")</caption>
            <column>USER_ID</column>
          </userField>
        </row>
      </subForm>
    </row>

    <row>
      <numberField>
        <name>initialQty</name>
        <udn>initialQty</udn>
