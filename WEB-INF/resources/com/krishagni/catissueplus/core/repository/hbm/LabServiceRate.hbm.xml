<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.biospecimen.domain.LabServiceRate" table="OS_LAB_SERVICE_RATES"
    entity-name="com.krishagni.catissueplus.core.biospecimen.domain.LabServiceRate">

    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">OS_LAB_SERVICE_RATE_SEQ</param>
      </generator>
    </id>

    <many-to-one name="rateList" column="RATE_LIST_ID" not-null="true" fetch="join" />

    <many-to-one name="service" column="SERVICE_ID" not-null="true" fetch="join" />

    <property name="rate" column="RATE" not-null="true" />
  </class>
</hibernate-mapping>